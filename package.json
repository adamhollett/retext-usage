{
  "name": "retext-usage",
  "version": "0.1.0",
  "description": "Warn about incorrect English usage with retext",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "array-differ": "^1.0.0",
    "array-intersection": "^0.1.2",
    "nlcst-normalize": "^1.1.0",
    "nlcst-search": "^1.1.1",
    "nlcst-to-string": "^1.0.0",
    "object-keys": "^1.0.7",
    "quotation": "^1.0.1",
    "unist-util-visit": "^1.0.0"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/admhlt/retext-usage.git"
  },
  "bugs": "https://github.com/admhlt/retext-usage/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "devDependencies": {
    "array-duplicated": "^1.0.1",
    "browserify": "^13.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.1",
    "istanbul": "^0.4.0",
    "js-yaml": "^3.3.1",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.0.0",
    "remark-comment-config": "^2.0.0",
    "remark-github": "^4.0.0",
    "remark-lint": "^2.0.0",
    "remark-slug": "^4.0.0",
    "remark-validate-links": "^2.0.0",
    "retext": "^1.0.0",
    "tap": "^5.0.0",
    "vfile-sort": "^1.0.0"
  },
  "scripts": {
    "build-bundle": "browserify index.js --no-builtins -s retextEquality > retext-equality.js",
    "build-mangle": "esmangle retext-equality.js > retext-equality.min.js",
    "build-extract": "node script/extract.js",
    "build-md": "remark . --quiet --frail",
    "build": "npm run build-md && npm run build-extract && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
